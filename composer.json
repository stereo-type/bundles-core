{
  "name": "slcorp/core-bundle",
  "version": "2.0.0",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "stability": "stable",
  "description": "A clean architecture Core Model bundle for Symfony.",
  "keywords": [
    "symfony",
    "core",
    "bundle",
    "clean architecture"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Zhalayletdinov Vyacheslav",
      "email": "evil_tut@mail.ru"
    }
  ],
  "require": {
    "php": ">=8.4",
    "doctrine/dbal": "^4.2.1",
    "doctrine/doctrine-bundle": "^2.13.1",
    "doctrine/orm": "^3.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/runtime": "^7.3",
    "symfony/yaml": "^7.3",
    "symfony/dotenv": "^7.3",
    "symfony/validator": "^7.3",
    "symfony/serializer": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/security-bundle": "^7.3",
    "symfony/flex": "^2.8.2",
    "phpdocumentor/reflection-docblock": "^5.6",
    "phpstan/phpdoc-parser": "^2.0",
    "symfony/property-info": "^7.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/phpunit-bridge": "^6.0",
    "phpstan/phpstan": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.64",
    "symfony/maker-bundle": "^1.61",
    "phpunit/phpunit": "^11.4",
    "phpstan/phpstan-doctrine": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan-phpunit": "^2.0",
    "symfony/browser-kit": "^7.1",
    "symfony/css-selector": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "Slcorp\\CoreBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slcorp\\CoreBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev",
      "stable": "master"
    },
    "symfony": {
      "allow-contrib": true,
      "recipe": "config/recipes",
      "bundles": {
        "SlCorp\\CoreBundle\\SlCorpCoreBundle": [
          "all"
        ]
      }
    }
  },
  "scripts": {
    "test": "phpunit",
    "cs-fix": "php-cs-fixer fix",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true
    }
  }
}
