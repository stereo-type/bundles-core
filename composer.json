{
  "name": "academcity/core-bundle",
  "version": "1.0.0",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "stability": "stable",
  "description": "A clean architecture Core Model bundle for Symfony.",
  "keywords": ["symfony", "core", "bundle", "clean architecture"],
  "license": "MIT",
  "authors": [
    {
      "name": "Zhalayletdinov Vyacheslav",
      "email": "evil_tut@mail.ru"
    }
  ],
  "require": {
    "php": ">=8.2",
    "doctrine/orm": "^3.3",
    "symfony/framework-bundle": "^7.1",
    "symfony/runtime": "^7.1",
    "symfony/yaml": "^7.1",
    "symfony/dotenv": "^7.1",
    "symfony/validator": "^7.1",
    "symfony/serializer": "^7.1",
    "symfony/property-access": "^7.1",
    "symfony/serializer-pack": "^1.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
    ,
    "symfony/phpunit-bridge": "^6.0",
    "phpstan/phpstan": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.64",
    "symfony/maker-bundle": "^1.61",
    "symfony/test-pack": "^1.0",
    "phpunit/phpunit": "^11.4",
    "phpstan/phpstan-doctrine": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan-phpunit": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "CoreBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CoreBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev",
      "stable": "master"
    },
    "symfony": {
      "allow-contrib": true,
      "recipe": "config/recipes"
    }
  },
  "scripts": {
    "test": "phpunit",
    "cs-fix": "php-cs-fixer fix"
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true,
      "phpstan/extension-installer": true
    }
  }
}
